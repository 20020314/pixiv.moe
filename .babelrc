{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
  	"transform-async-to-generator",
  	"transform-decorators-legacy",
    "transform-object-assign",
    "react-hot-loader/babel"
  ],
  "env": {
    "dist": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
