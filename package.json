    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
    "html-minifier": "^3.0.2",
    "imports-loader": "^0.6.5",
    "minimist": "^1.2.0",
    "mocha": "^3.0.0",
    "node-sass": "^3.10.0",
    "null-loader": "^0.1.1",
